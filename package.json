{
  "name": "chat-resource-center-javascript-materials",
  "description": "Materials which is used with Chat Resource Center to explain PubNub JS SDK usage.",
  "homepage": "https://github.com/pubnub/chat-examples-javascript#readme",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Serhii Mamontov <sergey@pubnub.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pubnub/chat-examples-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/pubnub/chat-examples-javascript/issues"
  },
  "main": "",
  "scripts": {
    "test": "jest --ci --forceExit",
    "test-examples": "jest --bail --ci --forceExit --runInBand --passWithNoTests --config=examples/jest-config.json",
    "test-snippets": "jest --bail --ci --forceExit --runInBand --config=snippets/jest-config.json",
    "lint": "gulp lint_all",
    "lint-snippets": "gulp lint_snippets",
    "lint-examples": "gulp lint_examples"
  },
  "dependencies": {
    "bignumber.js": "^8.1.1",
    "pubnub": "^4.23.0"
  },
  "devDependencies": {
    "react": "^16.8.6",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "dotenv": "^8.0.0",
    "eslint": "^5.15.2",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-shell": "^0.7.0",
    "jest": "^24.5.0",
    "jest-cli": "^24.5.0",
    "metro-react-native-babel-preset": "^0.53.1"
  },
  "jest": {
    "collectCoverage": false,
    "modulePathIgnorePatterns": [
      "<rootDir>/examples/",
      "<rootDir>/node_modules/(?!core-js|promise)/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.idea",
      "<rootDir>/examples/",
      "<rootDir>/node_modules/",
      "/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/{snippets,examples}/**/*.{js,jsx,mjs}"
    ],
    "notify": true
  },
  "keywords": [
    "chat",
    "resource",
    "center",
    "javascript",
    "pubnub"
  ]
}
